#!/bin/bash
###
 # @Description: Description
 # @Version: 1.0
 # @Author: luruoyang
 # @Date: 2023-03-29 23:23:12
 # @LastEditors: luruoyang
 # @LastEditTime: 2023-03-29 23:28:40
### 
name=$(basename $0) 
# 
if [ $name = "addem" ] 
then 
 total=$(($1 + $2 ))
# 
elif [ $name = "multem" ] 
then 
 total=$(( $1 * $2 ))
fi 
# 
echo 
echo The calculated value is $total 